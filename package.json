{
  "private": true,
  "name": "late",
  "version": "0.8.9",
  "description": "Lazy Automatic Time Evaluator",
  "main": "index.js",
  "engines": {
    "node": ">=11.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "bulma-steps": "^2.2.1",
    "bulma-switch": "^2.0.0",
    "bulma-tooltip": "^2.0.2",
    "cheerio": "^1.0.0-rc.2",
    "config": "^2.0.2",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "googleapis": "^36.0.0",
    "koa": "^2.6.2",
    "koa-body": "^4.0.4",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-respond": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-session": "^5.10.0",
    "koa-static": "^5.0.0",
    "koa2-cas": "^1.0.5",
    "moment": "^2.22.2",
    "mongoose": "^5.3.13",
    "node-cron": "^2.0.3",
    "npm": "^6.4.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "twilio": "^3.25.0",
    "vue": "^2.5.18",
    "vue-full-calendar": "^2.7.0",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.2",
    "vue-toasted": "^1.1.26",
    "vuex": "^3.0.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-prettier": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.18"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development node index.js",
    "test": "npm run lint",
    "serve": "cross-env PORT=8080 vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apexal/late.git"
  },
  "keywords": [
    "school",
    "time",
    "schedule",
    "education",
    "management"
  ],
  "author": "Frank Matranga <thefrankmatranga@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apexal/late/issues"
  },
  "homepage": "https://github.com/Apexal/late#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "standard",
      "plugin:vue/recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-multiple-empty-lines": [
        1,
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "no-trailing-spaces": [
        1,
        {
          "skipBlankLines": false
        }
      ],
      "no-mixed-spaces-and-tabs": [
        1
      ],
      "no-unused-vars": [
        1,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-console": [
        1,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "vue/component-name-in-template-casing": [
        "error",
        "PascalCase",
        {
          "ignores": [
            "router-view",
            "router-link",
            "transition",
            "transition-group",
            "keep-alive"
          ]
        }
      ],
      "vue/script-indent": "error",
      "vue/no-spaces-around-equal-signs-in-attribute": "error",
      "vue/no-unused-components": "off",
      "vue/no-unused-vars": "warning"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
